{
  "expo": {
    "name": "Daydate",
    "slug": "daydate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "daydate",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.daydate.app",
      "infoPlist": {
        "NSCameraUsageDescription": "Daydate needs camera access to capture your special moments together.",
        "NSPhotoLibraryUsageDescription": "Daydate needs photo library access to save and share your memories.",
        "NSLocationWhenInUseUsageDescription": "Daydate needs your location to create personalized date missions based on nearby places and activities.",
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleLocalizations": [
          "en",
          "ko"
        ],
        "CFBundleDevelopmentRegion": "en"
      }
    },
    "locales": {
      "ko": "./assets/locales/ko.lproj/InfoPlist.strings",
      "en": "./assets/locales/en.lproj/InfoPlist.strings"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.daydate.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Allow Daydate to access your camera to capture memories.",
          "enableMicrophonePermission": false
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Daydate to access your camera to capture memories."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Daydate to access your photos to share memories."
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow Daydate to use your location to create personalized date missions."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#FF6B9D"
        }
      ],
      "expo-asset",
      "expo-web-browser",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-9357146388578422~1720727384",
          "iosAppId": "ca-app-pub-9357146388578422~8753039591"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "115e6c85-54e5-4c9c-ac72-5714852ef605"
      },
      "router": {}
    },
    "owner": "usskslsksk"
  }
}
